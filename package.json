{
  "name": "stumpy-eleven",
  "version": "11.0.0",
  "description": "Stumptown Bear version eleven",
  "scripts": {
    "list-npm-modules": "npm access ls-packages krambuhl > build/data/npm-modules.json",
    "dev": "webpack-dev-server --hot --progress --config build/webpack.config.dev.js",
    "build": "webpack --progress --config build/webpack.config.build.js",
